<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context = "http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>

    <bean id = "person" class = "com.franktran.Person">
        <constructor-arg name="name" value="Frank"/>
        <constructor-arg name="age" value="28"/>
    </bean>

    <bean id="field" class="com.franktran.fields.Customer" />
    <bean id="setter" class="com.franktran.setters.Customer" />
    <bean id="constructor" class="com.franktran.constructors.Car" />
</beans>